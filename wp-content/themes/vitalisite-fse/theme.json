{
  "$schema": "https://schemas.wp.org/wp/6.9/theme.json",
  "customTemplates": [
    {
      "name": "template-links",
      "postTypes": ["page"],
      "title": "Page de liens"
    }
  ],
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "gradients": [
        {
          "gradient": "linear-gradient(135deg, #0B3D91, #6EC1E4)",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "gradient": "linear-gradient(135deg, #6EC1E4, #E0F5FC)",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "gradient": "linear-gradient(135deg, #0B3D91, #0F172A)",
          "name": "Calm",
          "slug": "calm"
        },
        {
          "gradient": "linear-gradient(135deg, #F1F5F9, #FFFFFF)",
          "name": "Light",
          "slug": "light"
        }
      ],
      "palette": [
        {
          "color": "#FFFFFF",
          "name": "Background",
          "slug": "bg"
        },
        {
          "color": "#F8FAFC",
          "name": "Surface",
          "slug": "surface"
        },
        {
          "color": "#F1F5F9",
          "name": "Surface 2",
          "slug": "surface-2"
        },
        {
          "color": "#E2E8F0",
          "name": "Border",
          "slug": "border"
        },
        {
          "color": "#0F172A",
          "name": "Text",
          "slug": "text"
        },
        {
          "color": "#64748B",
          "name": "Muted Text",
          "slug": "muted"
        },
        {
          "color": "#0B3D91",
          "name": "Primaire",
          "slug": "primary"
        },
        {
          "color": "#FFFFFF",
          "name": "Texte sur Primaire",
          "slug": "on-primary"
        },
        {
          "color": "#6EC1E4",
          "name": "Secondaire",
          "slug": "secondary"
        },
        {
          "color": "#0F172A",
          "name": "Texte sur Secondaire",
          "slug": "on-secondary"
        }
      ]
    },
    "custom": {
      "borderRadius": {
        "cta": "6px",
        "hero": "10px",
        "large": "10px",
        "medium": "8px",
        "small": "6px"
      },
      "fontWeight": {
        "bold": 700,
        "light": 300,
        "medium": 500,
        "regular": 400,
        "semi-bold": 600
      },
      "imageRadius": "8px",
      "lineHeight": {
        "body": 1.5,
        "loose": 2,
        "none": 1,
        "relaxed": 1.625,
        "snug": 1.2,
        "tight": 1.1
      }
    },
    "layout": {
      "contentSize": "56rem",
      "wideSize": "60rem"
    },
    "shadow": {
      "defaultPresets": false,
      "presets": [
        {
          "name": "Small",
          "shadow": "0px 1px 3px 0px rgba(15, 23, 42, 0.1), 0px 1px 2px 0px rgba(15, 23, 42, 0.06)",
          "slug": "small"
        },
        {
          "name": "Medium",
          "shadow": "0px 4px 6px -1px rgba(15, 23, 42, 0.1), 0px 2px 4px -1px rgba(15, 23, 42, 0.06)",
          "slug": "medium"
        },
        {
          "name": "Large",
          "shadow": "0px 10px 15px -3px rgba(15, 23, 42, 0.1), 0px 4px 6px -2px rgba(15, 23, 42, 0.05)",
          "slug": "large"
        },
        {
          "name": "Extra Large",
          "shadow": "0px 20px 25px -5px rgba(15, 23, 42, 0.1), 0px 10px 10px -5px rgba(15, 23, 42, 0.04)",
          "slug": "extra-large"
        }
      ]
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "2XS",
          "size": "0.25rem",
          "slug": "20"
        },
        {
          "name": "XS",
          "size": "0.5rem",
          "slug": "30"
        },
        {
          "name": "Small",
          "size": "clamp(0.75rem, 2vw, 1rem)",
          "slug": "40"
        },
        {
          "name": "Medium",
          "size": "clamp(1rem, 3vw, 1.5rem)",
          "slug": "50"
        },
        {
          "name": "Large",
          "size": "clamp(1.5rem, 4vw, 2rem)",
          "slug": "60"
        },
        {
          "name": "XL",
          "size": "clamp(2rem, 5vw, 3rem)",
          "slug": "70"
        },
        {
          "name": "2XL",
          "size": "clamp(3rem, 7vw, 4rem)",
          "slug": "80"
        }
      ],
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "typography": {
      "defaultFontSizes": false,
      "dropCap": false,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "system-ui, -apple-system, \"Segoe UI\", Roboto, sans-serif",
          "name": "Clinique Sans",
          "slug": "clinique-sans"
        },
        {
          "fontFamily": "Georgia, \"Times New Roman\", serif",
          "name": "Clinique Serif",
          "slug": "clinique-serif"
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "max": "0.95rem",
            "min": "0.8rem"
          },
          "name": "XS",
          "size": "0.875rem",
          "slug": "xs"
        },
        {
          "fluid": {
            "max": "1.05rem",
            "min": "0.95rem"
          },
          "name": "S",
          "size": "1rem",
          "slug": "sm"
        },
        {
          "fluid": {
            "max": "1.25rem",
            "min": "1.05rem"
          },
          "name": "M",
          "size": "1.125rem",
          "slug": "md"
        },
        {
          "fluid": {
            "max": "1.75rem",
            "min": "1.3rem"
          },
          "name": "L",
          "size": "1.5rem",
          "slug": "lg"
        },
        {
          "fluid": {
            "max": "2.75rem",
            "min": "1.9rem"
          },
          "name": "XL",
          "size": "2.25rem",
          "slug": "xl"
        },
        {
          "fluid": {
            "max": "3.5rem",
            "min": "2.4rem"
          },
          "name": "XXL",
          "size": "3rem",
          "slug": "xxl"
        }
      ],
      "writingMode": true
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "6px",
          "width": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--on-primary)"
        },
        "spacing": {
          "padding": {
            "bottom": "0.625rem",
            "left": "1.25rem",
            "right": "1.25rem",
            "top": "0.625rem"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600"
        }
      },
      "core/code": {
        "border": {
          "radius": "6px",
          "width": "0px"
        },
        "color": {
          "background": "var(--wp--preset--color--surface)",
          "text": "var(--wp--preset--color--text)"
        },
        "spacing": {
          "padding": {
            "bottom": "var(--wp--preset--spacing--40)",
            "left": "var(--wp--preset--spacing--40)",
            "right": "var(--wp--preset--spacing--40)",
            "top": "var(--wp--preset--spacing--40)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--monospace)",
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/details": {
        "css": ".wp-block-details summary { font-weight: 600; cursor: pointer; }"
      },
      "core/image": {
        "border": {
          "radius": "8px"
        }
      },
      "core/navigation": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "none"
              }
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "500"
        }
      },
      "core/paragraph": {
        "css": "&.has-background { padding: var(--wp--preset--spacing--40) }"
      },
      "core/post-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      },
      "core/quote": {
        "border": {
          "color": "var(--wp--preset--color--primary)",
          "radius": "0",
          "style": "solid",
          "width": "0 0 0 4px !important"
        },
        "css": "& :where(cite) { display: block }",
        "elements": {
          "cite": {
            "color": {
              "text": "var(--wp--preset--color--muted)"
            },
            "typography": {
              "fontSize": "var(--wp--preset--font-size--small)"
            }
          }
        },
        "spacing": {
          "margin": {
            "left": "0"
          },
          "padding": {
            "left": "var(--wp--preset--spacing--50)",
            "right": "var(--wp--preset--spacing--50)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "500",
          "lineHeight": "var(--wp--custom--line-height--body)"
        }
      },
      "core/site-tagline": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--40)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/site-title": {
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "letterSpacing": "0",
          "lineHeight": "var(--wp--custom--line-height--none)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--bg)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "button": {
        ":hover": {
          "color": {
            "background": "#0a3278"
          }
        },
        "border": {
          "radius": "6px",
          "width": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--on-primary)"
        },
        "spacing": {
          "padding": {
            "bottom": "0.625rem",
            "left": "1.25rem",
            "right": "1.25rem",
            "top": "0.625rem"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--line-height--tight)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--line-height--tight)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--snug)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--snug)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--base)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--body)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--line-height--body)"
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--clinique-sans)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "link": {
        ":hover": {
          "color": {
            "text": "#0a3278"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--70)",
      "padding": {
        "bottom": "0",
        "left": "0",
        "right": "0",
        "top": "0"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--clinique-sans)",
      "fontSize": "1.125rem",
      "fontWeight": "400",
      "lineHeight": "1.6"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "version": 3
}
